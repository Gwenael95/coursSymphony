{% extends "Home/index.html.twig" %}
{% block title %} {{ parent() }} - liste des projets{% endblock %}
{% block main %}
    <table class="table table-striped">
        <thead>
        <tr>
            <th scope="col">nom du projet</th>
            <th scope="col">id</th>
        </tr>
        </thead>
        <tbody>
        {% for project in projects %}
            <tr>
                <td>{{ project.name }}</td>
                <td>{{ project.projectId }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}